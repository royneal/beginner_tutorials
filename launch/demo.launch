
<launch>
     <arg name="record_chatter" default="true" />
    

    <arg name="rate" default="1" />
    <node pkg="beginner_tutorials" type="publisher_subscriber_node" name="publisher_subscriber_node" output="screen">
    <param name="txrate" value="$(arg rate)"/>
    </node>

     <node pkg="rosbag" type="record" name="rosbag_record_chatter"
       args="record  /chatter" output="screen"
       if="$(arg record_chatter)" />

  <!--  <node pkg="beginner_tutorials" name="client_node" type="client_node"/>-->
    <arg name="topics_name" default="joint_states /usb_cam/left/image_raw"/>

</launch>
